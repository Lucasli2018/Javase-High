package com.feiyangedu.sample;

import static java.lang.System.out;

public class Main {

	public static void main(String[] args) {
		Hello h = new Hello("World");
		out.println(h.hello());
	}

}

class Hello {

	private final String name;

	public Hello(String name) {
		this.name = name;
	}

	public String hello() {
		return "Hello, " + name + "!";
	}
}
