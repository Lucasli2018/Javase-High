<Configuration>
	<Appenders>
		<Console name="stdout">
			<PatternLayout pattern="%d %p [%t]: %m%n" />
		</Console>
		<File name="all">
			<PatternLayout pattern="%d %p [%t]: %m%n" />
		</File>
	</Appenders>
	<Loggers>
		<Root level="info">
			<AppenderRef ref="stdout" />
		</Root>
	</Loggers>
</Configuration>
